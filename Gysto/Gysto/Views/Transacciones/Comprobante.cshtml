@model Gysto.ViewModels.listadoTurnos
@{
    Layout = null;

}

<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
<div class="container">
    <br />
    <div class="row">
        <div class="col">
            <div style=" text-align:center;"><img src="https://cruba.com.ar/media/zoo/images/escudo-cordoba_f852878ec8d2b76d312259f305b058ad.png" widht="170" height="120" alt="..."> <br /> <i>Hospital Vicente Aguero</i></div>
        </div>
        <br />
        <hr />
        <h4>DATOS DEL TURNO</h4>
        <hr />
        <div class="row">
            <div class="col-sm-12">
                <table class="table table-borderless">
                    <thead>
                        <tr>
                            <th class="text-center">
                                Numero de turno
                            </th>
                            <th class="text-center">
                                Hora
                            </th>
                            <th class="text-center">
                                Fecha
                            </th>
                            <th class="text-center">
                                Medico
                            </th>
                        

                        </tr>

                    </thead>
                    <tbody>
                        

                            <tr class="text-center">
                                <td>
                                    @Model.id

                                </td>
                                <td>
                                    @Model.hora2()

                                </td>
                                <td>
                                    @Model.fecha2()
                                </td>
                                <td>
                                    @Model.medico
                                </td>
                           
                            </tr>
                        



                    </tbody>
                </table>


            </div>
        </div>
    </div>
</div>